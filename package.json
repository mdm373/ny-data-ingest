{
  "name": "ny-data-ingest",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean-temp": "rimraf .temp",
    "clean-dat": "rimraf .dat",
    "clean": "run-p clean-*",

    "dev": "tsc -w",
    "transpile": "tsc",
    
    "create-functions": "sh ./scripts/create-fns.sh",
    "get-data": "sh ./scripts/get-data.sh",
    
    "update-fn-create-tables" : "sh ./scripts/update-fn.sh create-tables",
    "update-fn" : "run-s update-fn-*",
    
    "deploy": "run-s clean-temp transpile update-fn",
    
    "create-tables": "aws lambda invoke --function-name nyc-data-ingest-create-tables ./.temp/create-tables-response.json",
    "sync-data": "node ./dist/tsc/sync-data", 
    "baseline": "run-s clean get-data create-functions deploy create-tables sync-data"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdm373/ny-data-ingest.git"
  },
